{
	"name": "social-media-site",
	"version": "1.1.0",
	"description": "Mock Social Media App Next.js with TypeScript / Husky (Lint-staged) / PM2 / ESLint / Prettier",
	"private": true,
	"author": {
		"name": "Jerren T",
		"email": "Jerren@trifall.com",
		"url": "https://trifall.com"
	},
	"contributors": [
		{
			"name": "Dolan Reynolds",
			"url": "https://dolan.dev"
		}
	],
	"scripts": {
		"dev": "next dev",
		"build": "yarn install && next build",
		"start": "next start",
		"lint": "eslint \"./src/**/*.{js,jsx,ts,tsx}\" --cache --fix --ignore-path ./.gitignore",
		"format": "prettier --ignore-path .gitignore --log-level warn --write \"./**/*.{js,jsx,ts,tsx,css,md,json}\" ",
		"pipeline": "yarn install && yarn format && yarn lint && next build",
		"prepare": "husky install",
		"generate": "drizzle-kit generate:sqlite --out ./drizzle/migrations --breakpoints --schema=./drizzle/schema.ts",
		"migrate": "tsx drizzle/migrate",
		"studio": "drizzle-kit studio --port 3333"
	},
	"license": "GPL-3.0-or-later",
	"dependencies": {
		"@headlessui/react": "^1.7.17",
		"@hookform/resolvers": "^3.3.2",
		"@libsql/client": "^0.3.5",
		"@nextui-org/framer-transitions": "^2.0.10",
		"@nextui-org/tooltip": "^2.0.24",
		"@tanstack/react-query": "^5.0.5",
		"@uploadthing/react": "^5.7.0",
		"axios": "^1.5.1",
		"drizzle-orm": "^0.28.6",
		"framer-motion": "^10.16.4",
		"next": "^13.5.6",
		"next-auth": "^4.24.3",
		"next-themes": "^0.2.1",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-hook-form": "^7.47.0",
		"react-icons": "^4.11.0",
		"sharp": "^0.32.6",
		"uploadthing": "^5.7.2",
		"zod": "^3.22.4"
	},
	"devDependencies": {
		"@tanstack/eslint-plugin-query": "^5.0.5",
		"@types/node": "^20.4.1",
		"@types/react": "^18.2.14",
		"@types/react-dom": "^18.2.6",
		"@typescript-eslint/eslint-plugin": "^6.0.0",
		"@typescript-eslint/parser": "^6.0.0",
		"autoprefixer": "^10.4.16",
		"better-sqlite3": "^9.0.0",
		"dotenv": "^16.3.1",
		"drizzle-kit": "^0.19.13",
		"eslint": "^8.44.0",
		"eslint-config-next": "^13.5.6",
		"eslint-config-prettier": "^8.8.0",
		"husky": ">=6",
		"lint-staged": ">=10",
		"postcss": "^8.4.31",
		"prettier": "^3.0.0",
		"tailwindcss": "^3.3.3",
		"tsx": "^3.14.0",
		"typescript": "5.2.2"
	},
	"packageManager": "yarn@1.22.19"
}
